{
"manifest_version": 3,
"name": "BackpackTF Page Jumper",
"version": "1.0.0",
"description": "Draggable UI to quickly set ?page= on backpack.tf classifieds without touching the URL bar.",

"content_scripts": [
{
"matches": [
"https://backpack.tf/classifieds*"
],
"js": ["content.js"],
"css": ["content.css"]
}
],
"icons": {
"16": "icons/icon16.png",
"32": "icons/icon32.png",
"48": "icons/icon48.png",
"128": "icons/icon128.png"
}

}